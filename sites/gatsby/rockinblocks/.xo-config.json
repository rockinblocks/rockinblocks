{
  "extends": ["xo", "xo-react"],
	"rules": {
    "object-curly-spacing": [1, "always"],
    "indent": 1,
    "quotes": [1, "double"],
    "react/prop-types": 0,
    "react/no-danger": 0,
    "react/no-unescaped-entities": 0,
		"semi": [2, "always"],
		"unicorn/filename-case": 0,
		"prefer-destructuring": ["error", {"object": true, "array": false}],
    "comma-dangle": 1
	}
}