{
  "name": "@rockinblocks/rockinblocks",
  "version": "0.1.0-alpha.1",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/@fontsource",
      "**/@fontsource/**"
    ]
  },
  "scripts": {
    "clean": "rm -rf node_modules packages/gatsby-plugin-rockinblocks/node_modules packages/gatsby-starter-rockinblocks/node_modules packages/gatsby-starter-rockinblocks/.cache  packages/gatsby-starter-rockinblocks/public",
    "dev": "yarn blocks:sb && yarn web:dev",
    "build": "yarn blocks:build && yarn web:build",
    "blocks:sb": "yarn workspace @rockinblocks/gatsby-plugin-rockinblocks storybook",
    "blocks:build": "yarn workspace @rockinblocks/gatsby-plugin-rockinblocks build",
    "blocks:test": "yarn workspace @rockinblocks/gatsby-plugin-rockinblocks jest",
    "web": "yarn workspace @rockinblocks/gatsby-starter-rockinblocks",
    "web:dev": "yarn workspace @rockinblocks/gatsby-starter-rockinblocks develop",
    "web:build": "yarn workspace @rockinblocks/gatsby-starter-rockinblocks build"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  }
}
