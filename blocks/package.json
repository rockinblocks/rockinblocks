{
  "name": "@rockinblocks/rockinblocks",
  "version": "0.1.0",
  "description": "Blocks for Rockin' Blocks",
  "author": "Jed Darrohn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:rockinblocks/rockinblocks.git"
  },
  "homepage": "https://rockinblocks.io",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "prebuild": "yarn lint --fix && yarn lint:prettier && yarn test:coverage",
    "build": "rimraf dist && cross-env NODE_ENV=production BABEL_ENV=production rollup -c",
    "build:dev": "rimraf dist && cross-env NODE_ENV=production BABEL_ENV=production rollup -c",
    "watch": "rimraf dist && cross-env NODE_ENV=production BABEL_ENV=production rollup -c -w",
    "test": "jest",
    "test:coverage": "jest --coverage --forceExit --colors",
    "lint": "yarn check-types && yarn xo src/**/**/*.tsx",
    "lint:prettier": "prettier src/**/**/*.tsx -w",
    "check-types": "tsc",
    "prepare": "yarn build",
    "prerelease": "yarn lint --fix && yarn test:coverage",
    "release": "yarn publish",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "gatsby": "^2.20.12",
    "react": "^16",
    "react-dom": "^16",
    "react-helmet": "^5.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.13.13",
    "@fontsource/poppins": "^4.2.2",
    "@fontsource/righteous": "^4.2.0",
    "@fontsource/roboto": "^4.2.0",
    "@fontsource/roboto-mono": "^4.2.0",
    "@rockinblocks/riff": "0.1.1-alpha",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-url": "^6.0.0",
    "@storybook/addon-actions": "^6.2.2",
    "@storybook/addon-essentials": "^6.2.2",
    "@storybook/addon-links": "^6.2.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.2",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^12.8.1",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "@types/react-helmet": "^5",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-plugin-remove-graphql-queries": "^3.2.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react-app": "^10.0.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "jasmine-expect": "^5.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "jest-pnp-resolver": "^1.2.2",
    "jest-resolve": "^26.6.2",
    "jest-watch-typeahead": "^0.6.1",
    "postcss": "^8",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^16",
    "react-app-polyfill": "^2.0.0",
    "react-docgen-typescript": "^1.21.0",
    "react-dom": "^16",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^16",
    "react-typography": "^0.16.20",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.32.11",
    "sass-loader": "^10.0.0",
    "source-map-loader": "^2.0.1",
    "storybook-css-modules-preset": "^1.0.8",
    "storybook-svgr-react-component": "^1.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "typography": "^0.16.21",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.js"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "typescript": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "transform-react-remove-prop-types"
    ],
    "exclude": "node_modules"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint-config-xo-react": "^0.25.0",
    "eslint-config-xo-typescript": "^0.41.1",
    "eslint-plugin-ava": "^12.0.0",
    "react-github-btn": "^1.2.0",
    "symlink-dir": "^5.0.1",
    "typescript-plugin-css-modules": "^3.2.0",
    "xo": "^0.40.1"
  }
}
